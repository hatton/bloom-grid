{
  "name": "bloom-grid",
  "version": "1.0.0",
  "description": "A TypeScript library for editing tables/grids in pure html",
  "main": "dist/bloom-grid.umd.js",
  "module": "dist/bloom-grid.es.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite --open /example.html",
    "build:watch": "vite build --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "grid",
    "table",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^24.0.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4"
  },
  "packageManager": "yarn@1.22.22",
  "volta": {
    "node": "22.11.0",
    "yarn": "1.22.22"
  }
}
